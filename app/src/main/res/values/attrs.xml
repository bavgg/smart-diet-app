<resources>
    <declare-styleable name="CustomCard">
        <attr name="head" format="string"/>
        <attr name="body" format="string"/>
        <attr name="cardIcon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomSelect">
        <attr name="icon" format="reference"/>
        <attr name="title" format="string"/>
        <attr name="selectValue" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomMealList">
        <attr name="mealImage" format="reference"/>
        <attr name="mealName" format="string"/>
        <attr name="calorie" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomExerciseList">
        <attr name="exerciseImage" format="reference"/>
        <attr name="exerciseName" format="string"/>
        <attr name="caloriesBurned" format="string"/>
    </declare-styleable>
</resources>
